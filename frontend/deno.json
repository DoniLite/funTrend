{
  "tasks": {
    "dev": "deno run -A npm:vite --host 0.0.0.0 --port 3000",
    "build": "deno run -A npm:vite build",
    "serve": "deno run -A npm:sirv dist --port 4000"
  },
  "imports": {
    "@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.6",
    "@std/assert": "jsr:@std/assert@1",
    "@tailwindcss/cli": "npm:@tailwindcss/cli@^4.0.17",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.4",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.4.1",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "lucide-react": "npm:lucide-react@^0.503.0",
    "react-dom": "npm:react-dom@18.3.1",
    "tailwind-merge": "npm:tailwind-merge@^3.2.0",
    "tailwindcss": "npm:tailwindcss@^4.0.17",
    "react": "npm:react@^18.3.1",
    "tw-animate-css": "npm:tw-animate-css@^1.2.7",
    "vite": "npm:vite",
    "@/": "./"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react",
    "jsxImportSourceTypes": "npm:@types/react",
    "lib": [
      "deno.window",
      "deno.ns",
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "DOM.AsyncIterable",
      "webworker"
    ]
  }
}
